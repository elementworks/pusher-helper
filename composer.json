    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "pusher helper"
    ],
    "support": {
        "docs": "https://github.com/elementworks/pusher-helper/blob/master/README.md",
        "issues": "https://github.com/elementworks/pusher-helper/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Element Works Ltd",
            "homepage": "https://elementworks.co.uk"
